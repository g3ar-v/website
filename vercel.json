{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/static/index.html"
        }
    ]
}